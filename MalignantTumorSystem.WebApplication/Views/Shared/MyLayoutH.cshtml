<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="Cache-Control" content="no-siteapp">
    <title>@ViewBag.Title</title>
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    @*<script src="~/Scripts/MyJs/H-ui_v3.0/lib/jquery/1.9.1/jquery.min.js"></script>*@
    <script src="~/Scripts/PCCSC/pccsc.js"></script>
    <script src="~/Scripts/Common/matchName.show.js"></script>
     <script src="~/Scripts/MyJs/jquery.autocomplete.js"></script>
    <link href="~/Scripts/MyJs/jqueryAutocomplete/jquery.autocomplete.css" rel="stylesheet" />
    <script src="~/Scripts/MyJs/My97DatePicker/WdatePicker.js"></script>
    <script src="~/Scripts/MyJs/H-ui_v3.0/static/h-ui/js/H-ui.min.js"></script>
    <link href="~/Scripts/MyJs/H-ui_v3.0/static/h-ui/css/H-ui.min.css" rel="stylesheet" />
    <!--[if lt IE 9]>
    <link href="~/Scripts/MyJs/H-ui_v3.0/static/h-ui/css/H-ui.ie.css" rel="stylesheet" />
    <![endif]--> 
    @RenderSection("head", required: false)
</head>
<body>
    <div>
        <form id="form1">
            <table class="table table-border table-bordered">
                <tr>
                    <td style="text-align:right">姓名<span style="color: #ff0000">*</span></td>
                    <td>
                        <input id="name" type="text" name="name" class="input-text radius size-S" style="width:100px;" />
                    </td>
                    <td style="text-align:right">性别<span style="color: #ff0000">*</span></td>
                    <td>
                        <label><input type="radio" name="sex" value="01" checked>男</label>
                        <label><input type="radio" name="sex" value="02">女</label>
                    </td>
                    <td style="text-align:right">身份证号码<span style="color: #ff0000">*</span></td>
                    <td>
                        <input type="text" id="id_card_number" name="id_card_number" class="input-text radius size-S" style="width:120px;" />
                    </td>
                    <td style="text-align:right">出生日期</td>
                    <td>
                        <input type="text" id="birth_date" name="birth_date" readonly="readonly" class="input-text radius size-S" style="width:100px;" />
                    </td>
                    <td style="text-align:right">年龄</td>
                    <td>
                        <input id="age" name="age" type="text" readonly="readonly" class="input-text radius size-S" style="width:80px;" />
                    </td>
                </tr>
                <tr>
                    <td>常住地址<span style="color: #ff0000">*</span></td>
                    <td colspan="5">
                        <select style="width: 80px" id="ddlProvince" name="ddlProvince" class="select" size="1">
                            <option value="">请选择</option>
                        </select>
                        <select style="width: 80px" id="ddlCity" name="ddlCity" class="select" size="1">
                            <option value="">请选择</option>
                        </select>
                        <select style="width: 80px" id="ddlCounty" name="ddlCounty" class="select" size="1">
                            <option value="">请选择</option>
                        </select>
                        <select style="width: 85px" id="ddlStreet" name="ddlStreet" class="select" size="1">
                            <option value="">请选择</option>
                        </select>
                        <select style="width: 105px" id="ddlCommunity" name="ddlCommunity" class="select" size="1">
                            <option value="">请选择</option>
                        </select>
                        <input type="text" id="perment_community_address" name="perment_community_address" class="input-text radius size-S" style="width:100px" />
                    </td>
                    <td>手机号码</td>
                    <td colspan="3">
                        <input type="text" id="txtIndividualPhone" name="txtIndividualPhone" class="input-text radius size-S" style="width:120px;" />
                        <input type="text" id="resident_id" name="resident_id" style="display: none" />
                    </td>
                </tr> 
            </table> 
            @RenderBody()
        </form> 
    </div>
</body>
</html>
